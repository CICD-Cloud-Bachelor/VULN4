{
  "ConnectionStrings": {
    "DefaultConnection": "server=${CONNECTION_STRING};port=3306;database=${DATABASE};user=${USERNAME};password=${PASSWORD}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "FeatureToggles": {
    "EnableNewFeatureX": true,
    "EnableBetaFeatureY": false
  },
  "ExternalServiceConfigurations": {
    "Service1": {
      "BaseUrl": "https://api.service1.com",
      "ApiKey": "service1_api_key"
    },
    "Service2": {
      "BaseUrl": "https://api.service2.com",
      "ApiKey": "service2_api_key"
    }
  }
}
